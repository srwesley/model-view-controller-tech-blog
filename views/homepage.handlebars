<!-- Loop over each object in the blog post array -->
{{#each blogPosts as |blogPost| }}
<div class="row mb-4 Project">
    <div class="col-md-5">
        <h2><a href="/blogPost/{{blogPost.id}}">{{blogPost.title}}</a></h2>
        <p>Created by {{blogPost
        .user.name}} on {{format_date blogPost
        .date_created}}</p>
    </div>
    <div class="col-md-7">
        <p>{{blogPost.contents}}</p>
    </div>
</div>
{{/each}}